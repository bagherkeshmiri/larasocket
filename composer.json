{
  "name": "bagherkeshmiri/larasocket",
  "description": "Simple, secure, native WebSocket server for Laravel (no external packages).",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Bagherkeshmiri\\LaraSocket\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Bagherkeshmiri\\LaraSocket\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "illuminate/support": "^10.0|^11.0"
  },
  "authors": [
    {
      "name": "Bagher Keshmiri",
      "email": "bagherkeshmiri@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Bagherkeshmiri\\LaraSocket\\LaraSocketServiceProvider"
      ]
    }
  }
}
